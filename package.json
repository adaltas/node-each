{
  "name": "each",
  "description": "Chained and parallel async iterator in one elegant function",
  "version": "1.2.2",
  "author": "David Worms <david@adaltas.com>",
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "level": "error",
      "value": "unix"
    },
    "max_line_length": {
      "level": "ignore"
    }
  },
  "contributors": [
    {
      "name": "David Worms",
      "email": "david@adaltas.com"
    }
  ],
  "dependencies": {},
  "devDependencies": {
    "coffeescript": "2.2.4",
    "mocha": "~5.0.5",
    "should": "~13.2.1"
  },
  "engines": {
    "node": ">= 0.9.0"
  },
  "homepage": "https://github.com/adaltas/node-each",
  "keywords": [
    "array",
    "asynchronous",
    "control flow",
    "each",
    "object"
  ],
  "license": "BSD-3-Clause",
  "main": "./lib/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/adaltas/node-each.git"
  },
  "scripts": {
    "preversion": "grep '## Trunk' CHANGELOG.md && rm -rf lib && npm test",
    "version": "version=`grep '^  \"version\": ' package.json | sed 's/.*\"\\([0-9\\.]*\\)\".*/\\1/'` && sed -i \"s/## Trunk/## Version $version/\" CHANGELOG.md && git add CHANGELOG.md",
    "postversion": "git push && git push --tags && npm publish",
    "patch": "npm version patch -m 'Bump to version %s'",
    "minor": "npm version minor -m 'Bump to version %s'",
    "major": "npm version major -m 'Bump to version %s'",
    "coffee": "./node_modules/.bin/coffee -b -o lib src",
    "pretest": "./node_modules/.bin/coffee -b -o lib src",
    "test": "./node_modules/.bin/mocha test/**/*.coffee"
  }
}
